{
  "extends": "@dcl/eslint-config/npm-package-json-lint",
  "rules": {
    "prefer-property-order": [
      "error",
      [
        "name",
        "version",
        "description",
        "main",
        "module",
        "types",
        "type",
        "exports",
        "files",
        "scripts",
        "dependencies",
        "peerDependencies",
        "peerDependenciesMeta",
        "devDependencies",
        "repository",
        "keywords",
        "author",
        "license",
        "bugs",
        "homepage",
        "engines",
        "overrides",
        "publishConfig"
      ]
    ]
  }
}
